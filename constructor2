public class constructor2 {
       int age;
       String name;
       constructor2(int age ,String name)
       {
    	   this.age=age;
    	   this.name=name;
    	   
       }
       void display()
       {
    	   System.out.println(age +" "+name);
       }
	public static void main(String[] args) {
		constructor2 obj=new constructor2(5,"Aju");
		obj.display();
		
	}

}
